{
  "expo": {
    "name": "Cupmakes",
    "slug": "bavery-dev-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#D53772"
    },
   
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#D53772"
      },
      "package": "com.mauriciogaraco.baverydevapp", 
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyBN1FTGbjsKDx_l-uLAvHGsofnd3FbXeOY6v"
        }
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "permissions": [
      "INTERNET"
    ],
    "plugins": [
      [
        "@stripe/stripe-react-native",
        {
          "enableGooglePay": true
        }
      ]
    
    ],
    "extra": {
      "eas": {
        "projectId": "a801122b-a85c-4a5f-a3c1-77c3b765bf76"
      }
    }
    
  }
}
